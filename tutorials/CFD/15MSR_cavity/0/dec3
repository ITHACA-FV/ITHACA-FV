FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      dec3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    movingWall
    {   
        type            zeroGradient;
	//value 		uniform 0;
    } 
    
    bottomWall
    {
        type            zeroGradient;
    }
    
    rightWall
    {
        type            zeroGradient;
    }
    
    leftWall
    {
        type            zeroGradient;
    }

    frontPlane
    {
        type            empty;
    }
    backPlane
    {
        type            empty;
    }
}


