FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// Simulation settings
startTime 0;
finalTime 7e-03;
timeStep 1e-06;
writeEvery 5e-05;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;
