FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
    volScalarFieldValue T 348.4
    volScalarFieldValue p 100000
);

regions
(
    boxToCell
    {
        box (0 0 -0.1) (5 1 0);
        fieldValues
        (
            volScalarFieldValue T 278.7
            volScalarFieldValue p 10000
        );
    }
);


