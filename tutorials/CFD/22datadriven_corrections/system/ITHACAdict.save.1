FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
NmodesU 10;
NmodesP 7;
NmodesSUP 7;
NmodesNUT 10;
NmodesProject 50;
NmodesMatrixRec 13;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;
