FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
//NmodesUout

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;


startTime 0;
finalTime 0.15;
writeEvery 0.0003;

NmodesUout 50;
//NmodePUout 50;
NmodesUproj 3;//4
