FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;
//EigenSolver spectra;

// Number of modes to output and to use for projection
NmodesUout 41;
NmodesUproj 16; //15

NmodesPout 41;
NmodesPproj 21;

NmodesDout 41;
NmodesDproj 1; //3

debug 1;
// Output format to save market vectors.
OutPrecision 20;
OutType fixed;

//bcMethod penalty;
warning false;
POD_pointDisplacement  Frobenius;

bcMethod      lift; 
/// ####### ldlt solver:
/// Nu = Np = 30, cpu time = 1.3023e+09
/// Nu = Np = 21, cpu time = 1.16318e+09
/// Nu = Np = 5, cpu time  = 8.95933e+08
/// Nu = Np = 11, cpu time = 9.76184e+08
/// Nu = 16,  Np = 21, cpu time = 1.10124e+09

// ##### colPivHH ######
// Nu =Np = 30, cpu time = 1344624761
// Nu =Np = 20, cpu time = 1.15692e+09
// Nu =Np = 5, cpu time  = 9.02333e+08
// Nu = Np= 15, cpu time = 1.06528e+09 
