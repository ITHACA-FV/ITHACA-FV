FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
NmodesTout 15;
NmodesTproj 10;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;
