FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      prec7;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 1e+19;

boundaryField
{
    movingWall
    {   
        type            fixedValue;
	value		uniform 0;
    } 
    
    bottomWall
    {
        type            fixedValue;
	value		uniform 0;
    }
    
    rightWall
    {
        type            fixedValue;
	value		uniform 0;
    }
    
    leftWall
    {
        type            fixedValue;
	value		uniform 0;
    }

    frontPlane
    {
        type            empty;
    }
    backPlane
    {
        type            empty;
    }
}



