#!/bin/bash
export LIB_ITHACA_ETC="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
export LIB_ITHACA="$(dirname "${LIB_ITHACA_ETC}")"
export LIB_ITHACA_SRC="$LIB_ITHACA/src"
export PYTHONPATH=$PYTHONPATH:${LIB_ITHACA_SRC}/python
export PATH=$PATH:${LIB_ITHACA}/bin
alias ithaca="cd $LIB_ITHACA" 

