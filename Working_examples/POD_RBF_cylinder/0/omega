FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 0 -1 0 0 0 0];

internalField uniform 1.6307298;

        boundaryField
        {
        cylinder 
        {
             type fixedValue;
             value uniform 1.6307298;
        }
         front
       {
           type empty;
       }
       back
       {
           type empty;
       }

       inlet {	type fixedValue;
      		value $internalField;      }
        outlet
         {
             type            inletOutlet;
             inletValue      $internalField;
             value           $internalField;

         }
           upAndDown
       {
           type zeroGradient;
       }
}


