FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 2 -2 0 0 0 0];

internalField uniform  0.000041704779;

boundaryField
{
       cylinder  
       {
           type fixedValue;
           value uniform 0;
       }
       
       front
       {
           type empty;
       }
       back
       {
           type empty;
       }

       inlet
     {
          type fixedValue;
           value $internalField;
      }
       outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

       upAndDown
       {
           type zeroGradient;
       }
     }


// ************************************************************************* //
